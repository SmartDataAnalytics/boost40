                                                                 rdfs:domain <http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Entity> ,
                                                                             <http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Object> ;
                                                                 rdfs:range <http://purl.oclc.org/NET/UNIS/fiware/iot-lite#Service> ;
